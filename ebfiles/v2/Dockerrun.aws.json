{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "semnil/bee-tutorial",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "ap-northeast-1",
          "awslogs-group": "/aws/elasticbeanstalk/bee-tutorial"
        }
      }
    }
  ]
}
